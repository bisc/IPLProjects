import Robot_Task_Types.*;
import tasks.*;

use view Library.i as lib;
use model prism_probs(INITIAL_LOCATION, TARGET_LOCATION, INITIAL_BATTERY );

stvar l: bool;    
  
set Lib : Library.i;  
set tasks : Task;     


// invalid
exists t1 : tasks | exists t2: tasks | 
	t1::task_type = 1 & t2::task_type = 0 & 
	t1::end_loc = t2::start_loc & 
	R{"time"}min=? [F (l = t1::start_loc)]{| 0, t2::end_loc, 10000 |} > 10;             
               
       
  